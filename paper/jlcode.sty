% defining the ucc and the ucclit command
% for literating special unicode characters
\newcommand{\ucc}[1]{%
\ifnum\lst@mode=\lst@Pmode\relax%
{\color{jlbase}#1}%
\else%
#1%
\fi%
}

\newcommand{\ucclit}[1]{%
\ifnum\lst@mode=\lst@Pmode\relax%
{\color{jlliteral}#1}%
\else%
